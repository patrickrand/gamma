{
    "agent_id": "my_agent",
    "host_id": "localhost",
    "checks": [
        {   
            "id": "py_check",
            "command": "/usr/bin/python /home/patrrand/py-check.py",
            "interval": 60,
            "alert_on": "ok/warning/critical",
            "handler_ids": ["h_stdout"]
        },
        {
            "id": "mem_check",
            "command": "/usr/bin/bash /home/patrrand/go-ws/src/github.com/patrickrand/gamma/monitor/checks/monitor-mem.sh -c 90 -w 80",
            "interval": 60,
            "alert_on": "ok/warning/critical",
            "handler_ids": ["h_stdout"]
        }
    ],     
    "handlers": [ 
        {
            "id": "http_handler_1",
            "type": "http",
            "destination": "localhost:8080",                
            "parameters": {}
        },
        {
            "id": "file_handler_1",
            "type": "file",
            "destination": "stdout",
            "parameters": {}
        }
    ]
}


